{
  "version": "0.1",
  "publicReleaseRefSpec": [
    "^refs/heads/main$",
    "^refs/heads/develop$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  },
  "branches": {
    "main": {
      "versionIncrement": "minor",
      "firstUnstableTag": null,
      "tag": "v{version}"
    },
    "develop": {
      "baseVersion": "0.2",
      "versionIncrement": "patch",
      "firstUnstableTag": "preview.{height}",
      "tag": "v{version}"
    }
  }
}
